<Window x:Class="Match3PlusUltraDeluxEX.ResultsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Match3PlusUltraDeluxEX"
        mc:Ignorable="d"
        Title="ResultsWindow" Height="720" Width="1080">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <TextBlock x:Name="FinalScore" Grid.Row="0" Margin="10" FontSize="32"/>
        <Button Grid.Row="1" FontSize="24" Content="Main menu" Click="BackInMenu"/>
    </Grid>
</Window>
